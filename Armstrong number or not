n=int(input())
for i in range(n):
    a=int(input())
    temp=a
    sum1=0
    while(a):
        b=a%10
        sum1+=b**3
        a=a//10
    if(sum1==temp):
        print("armstrong number")
    else:
        print("Not armstrong number")
        
'''
TEST CASE 1:
2
370
1500

OUTPUT:
armstrong number
Not armstrong number
'''
