n=int(input())
x=n//2
if(n%2!=0):
    for i in range(n):
        for j in range(n):
            if i+j==x or j-i==x or i-j==x or i+j==x*3:
                print("*",end="")
            else:
                print(end=" ")
        print()

'''
 OUTPUT:   
               *   
              * *  
             *   * 
            *     *
           *       *
            *     * 
             *   *  
              * *
               *
'''
